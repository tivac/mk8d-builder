{
  "name": "mk8deluxe",
  "version": "1.3.1",
  "description": "Mario Kart 8 Deluxe Kart Builder",
  "main": "index.js",
  "keywords": [],
  "author": "Pat Cavit <npm@patcavit.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "buble-loader": "^0.4.1",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "cross-var": "^1.0.3",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-config-arenanet": "^3.3.1",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.28.0",
    "modular-css-namer": "^5.1.2",
    "modular-css-webpack": "^5.2.2",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "mithril": "^1.1.1"
  },
  "scripts": {
    "predeploy": "webpack -p --env=\"dist\"",
    "deploy": "cross-var \"gh-pages --dist dist --message=\\\"v$npm_package_version\\\"\"",
    "lint": "eslint .",
    "start": "webpack-dev-server",
    "version": "npm run deploy",
    "postversion": "git push --follow-tags",
    "prerelease": "npm run lint",
    "release": "npm version -m \"v%s\""
  },
  "eslintConfig": {
    "extends": "arenanet",
    "env": {
      "node": true,
      "browser": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "indent": "off"
    }
  }
}
